      <field name="model">chricar.budget</field>
      <field name="inherit_id" ref="chricar_budget.chricar_budget_form"/>
      <field name="arch" type="xml">
        <xpath expr="/form/notebook/page[@string='Notes']" position="before">
          <page string="Sales">
            <field name="budget_lines_sales_ids" widget="one2many_list" colspan="4" nolabel="1">
              <tree string="Sales">
                <field
                  name="date_planning"
                  on_change="on_change_date(parent.budget_version_id,parent.product_id,parent.price_unit_id,date_planning,payment_term_id)"
                  />
                <field name="date_due" />
                <field name="partner_id" />
                <field
                  colspan="4"
                  name="quantity"
                  on_change="on_change_quantity(quantity,price_unit,price_unit_id, amount_bom)"
                  />
                <field
                  name="price_unit"
                  on_change="on_change_quantity(quantity,price_unit,price_unit_id, amount_bom)"/>
                <field
                  name="price_unit_id"
                  on_change="on_change_quantity(quantity,price_unit,price_unit_id, amount_bom)"/>
                <field name="amount_sales"  sum="Sum Amount Sales"/>
                <field name="bom_id" />
                <field
                  name="amount_bom"
                  on_change="on_change_bom(amount_sales, amount_bom)"
                  />
                <field name="amount"  sum="Sum Amount"/>
                <field name="name" />
                <field name="state" />
              </tree>
              <form string="Sales">
                <group colspan="2" col="4">
                  <separator string="Dates" colspan="4"/>
                  <field
                    name="date_planning"
                    on_change="on_change_date(parent.budget_version_id,parent.product_id,parent.price_unit_id,date_planning,payment_term_id)"
                    required="1"
                    />
                  <field name="period_id"  required="1"/>
                  <field name="date_due"  required="1"/>
                  <field name="payment_term_id" />
                  <separator string="Values" colspan="4"/>
                  <field name="name"  colspan="4"/>
                  <field
                    name="quantity"
                    on_change="on_change_quantity(quantity,price_unit,price_unit_id, amount_bom)"
                    />
                  <newline/>
                  <field
                    name="price_unit"
                    on_change="on_change_quantity(quantity,price_unit,price_unit_id, amount_bom)"/>
                  <field
                    name="price_unit_id"
                    on_change="on_change_quantity(quantity,price_unit,price_unit_id, amount_bom)"/>
                  <field name="amount_sales" />
                  <field name="state" />
                  <newline/>
                  <field
                    name="amount_bom"
                    on_change="on_change_bom(amount_sales, amount_bom)"
                    />
                  <field name="bom_id" />
                  <field name="amount" />
                  <field name="currency_id" />
                </group>
                <group colspan="2" col="2">
                  <field name="partner_id" />
                  <field name="analytic_account_id" />
                  <field name="budget_version_id" />
                  <field
                    domain="[('type','=','normal')]"
                    name="budget_item_id"
                    />
                  <separator string="Notes" colspan="2"/>
                  <field name="notes"  nolabel="1" colspan="2"/>
                </group>
              </form>
            </field>
          </page>
          <page string="Production">
            <field name="budget_lines_production_ids" widget="one2many_list" colspan="4" nolabel="1">
              <tree string="Production Costs">
                <field
                  name="date_planning"
                  on_change="on_change_date(parent.budget_version_id,date_planning,payment_term_id)"
                  required="1"
                  />
                <field name="date_due"  required="1"/>
                <field name="name" />
                <field
                  name="quantity_base"
                  on_change="on_change_quantity_base(quantity_base, parent.surface, parent.product_qty, price_unit,price_unit_id,price_factor,amount_bom)"
                  />
                <field name="product_id"/>
                <field
                  name="quantity"
                  on_change="on_change_quantity(quantity,price_unit,price_unit_id,price_factor,amount_bom)"
                  />
                <field
                  name="price_unit"
                  on_change="on_change_quantity(quantity,price_unit,price_unit_id,price_factor,amount_bom)"/>
                <field
                  name="price_unit_id"
                  on_change="on_change_quantity(quantity,price_unit,price_unit_id,price_factor,amount_bom)"/>
                <field name="amount_production"  sum="Amount Production"/>
                <field name="bom_id" />
                <field
                  name="price_factor"
                  on_change="on_change_bom(amount_production,price_factor,amount_bom)"
                  />
                <field
                  name="amount_bom"
                  on_change="on_change_bom(amount_production,price_factor,amount_bom)"
                  />
                <field name="amount_cost"  sum="Amount Cost"/>
                <field name="location_id" />
                <field name="partner_id" />
                <field name="state" />
              </tree>
              <form string="Production Costs">
                <group colspan="2" col="4">
                  <separator string="Dates" colspan="4"/>
                  <field
                    name="date_planning"
                    on_change="on_change_date(parent.budget_version_id,date_planning,payment_term_id)"
                    />
                  <field name="period_id" />
                  <field name="date_due" />
                  <field name="payment_term_id" />
                  <separator string="Values" colspan="4"/>
                  <field name="product_id"  colspan="4"/>
                  <field name="name"  colspan="4"/>
                  <field
                    name="quantity"
                    on_change="on_change_quantity(quantity,price_unit,price_unit_id,price_factor,amount_bom)"
                    />
                  <field
                    name="quantity_base"
                    on_change="on_change_quantity_base(quantity_base, parent.surface, parent.product_qty, price_unit,price_unit_id,price_factor,amount_bom)"
                    />
                  <field
                    name="price_unit"
                    on_change="on_change_quantity(quantity,price_unit,price_unit_id,price_factor,amount_bom)"/>
                  <field name="price_unit_id"/>
                  <field name="amount_production" />
                  <field name="state" />
                  <newline/>
                  <field name="bom_id"  colspan="4"/>
                  <field
                    name="amount_bom"
                    on_change="on_change_bom(amount_production,price_factor,amount_bom)"
                    />
                  <field
                    name="price_factor"
                    on_change="on_change_bom(amount_production,price_factor,amount_bom)"
                    />
                  <field name="amount_cost" />
                  <field name="currency_id" />
                  <field name="amount" invisible="1"/>
                </group>
                <group colspan="2" col="2">
                  <field name="location_id" />
                  <field name="partner_id" />
                  <field name="analytic_account_id" />
                  <field name="budget_version_id" />
                  <field
                    domain="[('type','=','normal')]"
                    name="budget_item_id"
                    />
                  <separator string="Notes" colspan="2"/>
                  <field name="notes"  nolabel="1" colspan="2"/>
                </group>
              </form>
            </field>
          </page>
        </xpath>
      </field>
    </record>
