<?xml version='1.0' encoding='utf-8'?>
<openerp>
    <data>
        
        <record model="ir.ui.view" id="view_res_partner_form_inh_accounts">
            <field name="name">view.res.partner.form.inh.accounts</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='property_account_payable']" position="replace">
                        <field name="property_account_payable"/>
                </xpath>
                <xpath expr="//field[@name='property_account_receivable']" position="replace">
                        <field name="property_account_receivable"/>
                </xpath>
            </field>
        </record>
        
    </data>
</openerp>
