return node;}).compact().uniq().each(function(node){$(node).trigger('content_changed');})});function remove_oe_attributes(nodes){_.chain(nodes).filter(function(node){return node.nodeType===1}).each(function(node){_.each(_.toArray(node.attributes),function(attr){if(attr.nodeName.indexOf('data-oe-')===0){node.removeAttribute(attr.nodeName)}});remove_oe_attributes(node.childNodes);});};function remove_mundane_nodes(nodes){if(!nodes||!nodes.length){return[];}
