  <story>
    <para style="terp_default_8">[[repeatIn(objects,'o')]]</para>
    <para style="terp_default_8">[[ setLang(o.partner_id.lang) ]]</para>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="270.0,270.0" style="Table_naslov">
      <tr>
        <td>
          <para style="P10">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P1">[[ o.state not in ['draft','sent'] and removeParentNode('para') ]] Quotation N° [[ o.name ]]</para>
          <para style="P1">[[ o.state in ['draft','sent'] and removeParentNode('para') ]] Order N° [[ o.name ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="264.0,276.0" style="Table_customer_data">
      <tr>
        <td>
          <para style="P3">[[ (o.partner_id and o.partner_id.title and o.partner_id.title.name) or '' ]] [[ (o.partner_id and o.partner_id.name) or '' ]] </para>
          <para style="P3">[[ (o.partner_id.street) or '' ]]</para>
          <para style="P3">
            <font color="white"> </font>
          </para>
          <para style="P3">[[ (o.partner_id.zip) or '' ]] [[ (o.partner_id.city) or '' ]]</para>
          <para style="P3">[[ (o.partner_id.country_id.name) or '' ]]</para>
          <para style="P3">
            <font color="white"> </font>
          </para>
          <para style="P2">[[ (o.partner_id == o.partner_shipping_id) and removeParentNode('para') ]] Shipping address :</para>
          <para style="P3">[[ ((o.partner_id &lt;&gt; o.partner_shipping_id) and (o.partner_shipping_id and o.partner_id.title and o.partner_shipping_id.title.name)) or removeParentNode('para') ]]</para>
          <para style="P3">[[ ((o.partner_id &lt;&gt; o.partner_shipping_id) and o.partner_shipping_id and o.partner_shipping_id.name) or removeParentNode('para') ]] </para>
          <para style="P3">[[ (o.partner_id &lt;&gt; o.partner_shipping_id) and (o.partner_shipping_id and display_address(o.partner_shipping_id)) or removeParentNode('para') ]]</para>
          <para style="P3">
            <font color="white"> </font>
          </para>
          <para style="P2">[[ o.partner_id == o.partner_invoice_id and removeParentNode('para') ]] Invoice address :</para>
          <para style="P3">[[ ((o.partner_id &lt;&gt; o.partner_invoice_id) and (o.partner_invoice_id and o.partner_id.title and o.partner_invoice_id.title.name)) or removeParentNode('para') ]]</para>
          <para style="P3">[[ ((o.partner_id &lt;&gt; o.partner_invoice_id) and o.partner_invoice_id and o.partner_invoice_id.name) or removeParentNode('para') ]]</para>
          <para style="P6">[[ ((o.partner_id &lt;&gt; o.partner_invoice_id) and o.partner_invoice_id and display_address(o.partner_invoice_id)) or removeParentNode('para') ]]</para>
        </td>
        <td>
          <blockTable colWidths="108.0,162.0" style="Table_order_inf">
            <tr>
              <td>
                <para style="P4">Place, date:</para>
              </td>
              <td>
                <para style="P4">Dravograd, <font face="Helvetica">[[ formatLang(o.date_order, date = True) ]]</font></para>
              </td>
            </tr>
            <tr>
              <td>
                <para style="P4">Confirmation date:</para>
              </td>
              <td>
                <para style="P8">[[ formatLang(o.date_confirm, date = True) ]]</para>
              </td>
            </tr>
            <tr>
              <td>
                <para style="P4">Order reference:</para>
              </td>
              <td>
                <para style="P4">[[ o.sale_order_ref ]]</para>
              </td>
            </tr>
            <tr>
              <td>
                <para style="P4">Customer reference:</para>
              </td>
              <td>
                <para style="P4">[[ o.client_order_ref ]]</para>
              </td>
            </tr>
          </blockTable>
          <para style="P10">
            <font color="white"> </font>
          </para>
        </td>
      </tr>
    </blockTable>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="314.0,226.0" style="Table_davek">
      <tr>
        <td>
          <para style="P5">VAT: [[ (o.partner_id and o.partner_id.vat) or removeParentNode('para') ]]</para>
