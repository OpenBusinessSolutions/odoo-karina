  <story>
    <para style="Standard">[[setLang(user.context_lang)]]</para>
    <para style="Standard">
      <font color="white"> </font>
    </para>

    <para style="P12">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="100.0,441.0" style="Table2" repeatRows="1">
