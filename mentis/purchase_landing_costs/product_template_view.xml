<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="product_landing_cost_view">
            <field name="name">product.landing.cost.view</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <field name="price_extra" position="after">
                    <group name="landing_costs" string="Landing Costs">
                        <field name="landing_cost"/>
                        <field name="landing_cost_calculate"/>
                    </group>
                </field>
            </field>
    	</record>
    </data>
</openerp>
